<h4 class="pt-3">ユーザー一覧</h4>
<% @users.each do |user| %>
<div class="container">
  <div class="row p-2">
    <div class="col-2">
      <%= link_to user_path(user.id) do %>
      <%= user.name %>
      <% end %>
    </div>
    <div class="col-2">
      <%= user.email %>
    </div>
  </div>
</div>
<% end %>